{templateType Stepapo\Dataset\Control\ItemList\ItemListTemplate}

<ul n:class="$itemListClass" n:snippet>
	{if $control->getDatasetControl()->getCurrentCount() > 0}
		{foreach $items as $item}
			{control 'item-' . $item->getValue($idColumnName)}
		{/}
	{else}
		<li>
			{_$text->noResults}
		</li>
	{/}
</ul>
